{% extends 'base.html.twig' %}

{% block title %}Hello DefaultController!{% endblock %}

{% block body %}
    <h3>Liste de meme</h3>
    <ul class="list-group w-50">
        {% for meme in memes%}
            <a href="{{ meme.link }}" target="_blank"><div class="list-group-item list-group-item-action rounded">{{ meme.name }} | {{ meme.createdAt | date('d/m/Y') }}</div></a>
        {% endfor %}
    </ul>

{% endblock %}
